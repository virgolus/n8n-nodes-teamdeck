{
    "name": "n8n-nodes-teamdeck",
    "version": "1.0.0",
    "description": "N8N node to interact with Teamdeck API",
    "keywords": [
        "n8n-community-node-package",
        "teamdeck"
    ],
    "license": "MIT",
    "homepage": "",
    "author": {
        "name": "Ezio Vergine",
        "email": "virgolus@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/virgolus/n8n-nodes-teamdeck.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "npx tsc && gulp build",
        "dev": "tsc --watch",
        "format": "prettier --write .",
        "lint": "eslint src nodes --ext .ts",
        "prepare": "npm run build"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/TeamdeckApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Teamdeck/Teamdeck.node.js"
        ]
    },
    "devDependencies": {
        "@langchain/core": "^0.3.18",
        "@types/express": "^4.17.6",
        "@types/luxon": "^3.0.0",
        "@types/node": "^20.11.1",
        "@types/request-promise-native": "~1.0.15",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.28.0",
        "eslint-plugin-n8n-nodes-base": "^1.0.0",
        "gulp": "^4.0.2",
        "gulp-rename": "^2.0.0",
        "module-alias": "^2.2.2",
        "prettier": "^2.7.1",
        "typescript": "~5.3.0",
        "n8n-core": "^1.5.0",
        "n8n-workflow": "^1.5.0",
        "@types/node-fetch": "^2.6.11",
        "@sentry/node": "^7.0.0",
        "@types/nock": "^11.1.0"
    },
    "dependencies": {
        "request": "^2.88.2",
        "request-promise-native": "^1.0.9"
    },
    "peerDependencies": {
        "n8n-core": "^1.5.0",
        "n8n-workflow": "^1.5.0"
    },
    "_moduleAliases": {
        "@": "dist"
    }
}
